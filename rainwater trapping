#include<iostream>
using namespace std;
int maxiwater(int arr[], int n){
	int water=0;
	for(int i=0; i<n-1; i++){
		int left = arr[i];
		for(int j=0; j<i; i++)
		right = max(right, arr[j]);
		water = water +(min(left, right)-arr[i]);
	}
	return res;
}
int main(){
	int arr[]= {8, 1, 7, 5, 3, 6, 8, 9};
	int n= sizeoff(arr)/sizeoff(arr[0])
